{
	"compilerOptions": {
		"allowJs": true,
		"declaration": false,
		"jsx": "react",
		"jsxFactory": "dom.h",
		"strict": true,
		"strictNullChecks": true,
		"noUnusedLocals": true,
		"noImplicitThis": true,
		"allowUnusedLabels": true,
		"module": "ES6",
		"moduleResolution": "node",
		"sourceMap": true,
		"target": "es2017",
		"downlevelIteration": false,
		"types": [],
		"lib": [
			"es5",
			"dom",
			"es2015.promise",
			"preact"
		],
		"paths": {
		}
	}
}
{
	"compilerOptions": {
		"allowJs": false,
		"declaration": true,
		"declarationDir": "types",
		"jsx": "react",
		"jsxFactory": "dom.h",
		"strict": true,
		"strictNullChecks": true,
		"noUnusedLocals": true,
		"noImplicitThis": true,
		"allowUnusedLabels": true,
		"module": "ES6",
		"moduleResolution": "node",
		"sourceMap": true,
		"target": "es5",
		"downlevelIteration": false,
		"types": [],
		"lib": [
			"es5",
			"dom",
			"es2015.promise"
		]
	}
}